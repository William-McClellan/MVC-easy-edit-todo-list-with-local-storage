displayProj(projects){
        while(this.projects.firstChild){
            this.projects.removeChild(this.projects.firstChild)
        }    
            this.newProjBut = this.createElement('button');
            this.newProjBut.textContent = 'New Project';

            this.projDiv.append(this.newProjBut);

            this.newProjBut.prepend(projects);

            projects.forEach(project =>{
                this.
            })
    }
